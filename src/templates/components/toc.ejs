<% if (toc && toc.length > 0) { %>
  <nav class="retro-toc">
    <h3>Table of Contents</h3>
    <ul>
      <% toc.forEach(function(item) { %>
        <li class="toc-level-<%= item.level %>">
          <a href="#<%= item.id %>"><%= item.text %></a>
        </li>
      <% }); %>
    </ul>
  </nav>
<% } %>
