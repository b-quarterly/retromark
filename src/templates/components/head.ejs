<meta charset="UTF-8">
<meta name="viewport" content="width=device-width, initial-scale=1.0">
<meta name="description" content="<%= config.description %>">
<meta name="author" content="<%= config.author %>">
<link rel="stylesheet" href="<%= config.baseUrl %>style.css">
<% if (config.custom && config.custom.css) { %>
  <link rel="stylesheet" href="<%= config.baseUrl %><%= config.custom.css %>">
<% } %>
<% if (config.custom && config.custom.favicon) { %>
  <link rel="icon" href="<%= config.baseUrl %><%= config.custom.favicon %>">
<% } %>
